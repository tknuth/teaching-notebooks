   "source": [
    "def number_of_lists(n, i=0):\n",
    "    if n == 1:\n",
    "        return 1\n",
    "    return sum([\n",
    "        1, # all n items form 1 list\n",
    "        2 * number_of_lists(n-1, i), # new item can be added on both sides\n",
    "        -1 # don't list of lists of length 1 twice\n",
    "    ])\n",
    "\n",
    "number_of_lists(4) # 2^(n-1)"
   ]
  }
 ],
